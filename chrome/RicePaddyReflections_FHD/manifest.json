{
  "manifest_version": 3,
  "name": "Rice Paddy Reflections",
  "version": "1.0",
  "description": "A refreshing Chrome theme capturing the serenity of rice paddies reflecting a vast blue sky with fluffy clouds and distant mountains.",
  "theme": {
    "images": {
      "theme_ntp_background": "theme_ntp_background.jpg"
    },
    "colors": {
      "frame": [65, 125, 195],
      "frame_inactive": [100, 145, 200],
      "ntp_background": [80, 140, 210],
      "tab_background_text": [180, 210, 240],
      "tab_text": [20, 20, 25],
      "bookmark_text": [30, 60, 100],
      "ntp_text": [30, 60, 100],
      "ntp_link": [20, 80, 160],
      "ntp_header": [50, 90, 170],
      "toolbar": [220, 235, 250],
      "toolbar_text": [30, 60, 100],
      "toolbar_button_icon": [50, 100, 180],
      "omnibox_text": [30, 60, 100],
      "omnibox_background": [240, 250, 255]
    },
    "tints": {
      "buttons": [0.6, 0.5, 0.7]
    },
    "properties": {
      "ntp_background_alignment": "center",
      "ntp_background_repeat": "no-repeat"
    }
  }
}